@model IEnumerable<ROMS.Bussiness.DTOs.ViewCategories>@{    ViewData["Title"] = "Index";}<div class="treeview">    @foreach (var item in Model)
    {        <h2>@item.categoryname</h2>        <ul id="@item.categoryname">            @foreach (var item2 in item.subcategories)
            {                <h4>@item2.SubCategoryName</h4>                <ul id="subcategory_@item2.SubCategoryName">                    @foreach (var item3 in item2.menuitems)
                    {                        <div class="row-cols-4 menu-header">                            <div class="item col align-items-center">                                <div class="col-12 col-md-2 text-center mb-3 mb-md-0 data22 image-container">                                    <img src="~/@item3.Image" class="img-fluid" id="preview">                                    <span class="d-inline d-md-block no-wrap" id="name-@item3.Id">@item3.name</span>                                </div>                                <div class="col-12 col-md-5 text-center img-2" style="justify-content: space-between;">                                    <div class="description" id="descriptionText"></div>                                    <i class="bi bi-exclamation-circle-fill" style="font-size: 2em;" title="@item3.Description" datas="tooltip"></i>                                </div>                                <div class="col-12 col-md-3 text-center">                                    <div class="price-container">                                        <i class="bi bi-currency-rupee">                                        </i>                                        <span class="data" id="price-@item3.Id" data-price="@item3.price">@item3.price</span>                                    </div>                                </div>                                <div class="col-12 col-md-4 text-center">                                    <div class="counter-container">                                        <button onclick="decrement('@item3.Id','price-@item3.Id')" class="btn btn-sm btn-dark" style="border-radius: 50px;">-</button>                                        <span class="quantity" id="counter-@item3.Id" data-price="@item3.price">0</span>                                        <button onclick="increment('@item3.Id','price-@item3.Id')" class="btn btn-sm btn-dark" style="border-radius: 50px;">+</button>                                    </div>                                </div>                            </div>                        </div>                    }                </ul>            }        </ul>    }</div><div class="row-md-6 sticky-button-container mr ml-auto text-right text-md-right">    <a onclick="addToCart()" style="font-size:x-large; border-radius:50px; position:sticky" class="btn btn-lg btn-dark tt mr" href="/CartPage/Index"><i class="bi bi-arrow-right-circle"></i> Next</a></div>@section Scripts {    <script src="~/Client/js/menuitem.js">    </script>}